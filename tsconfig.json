{
"extends": "astro/tsconfigs/strict",
"include": [".astro/types.d.ts", "**/*"],
"exclude": ["dist"],
  "compilerOptions": {
    "paths": {
      "@config/*": [
        "src/config/*"
      ],
      "@types": [
        "src/types/index.ts"
      ],
      "@types/*": [
        "src/types/*"
      ]
    }
  }
}
